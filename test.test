                }
            };

            //{container:<div id>, symbol:<symbol to load>}

            EDGRSTX.initialize=function(params){
                if(params.symbol) EDGRSTX.config.symbol=params.symbol;
                if(params.range) EDGRSTX.config.range=params.range;
                if(params.comparison) EDGRSTX.config.comparison=params.comparison;
                EDGRSTX.iframe=document.createElement("IFRAME");
                EDGRSTX.iframe.style.width="100%";
                EDGRSTX.iframe.style.height="100%";
                EDGRSTX.iframe.src=EDGRSTX.config.src;
                EDGRSTX.iframe.onload=function(){
                    EDGRSTX.iframe.contentWindow.postMessage(JSON.stringify(EDGRSTX.config), "*");		
                }

                var node=document.getElementById(params.container);
                    node.appendChild(EDGRSTX.iframe);
            };

            EDGRSTX.resetToDefault=function(){
                EDGRSTX.iframe.contentWindow.postMessage(JSON.stringify({action:"resetToDefault"}), "*");
            };

            /* comparisons should be an array of symbols */
            EDGRSTX.setComparisons=function(comparisons){
                EDGRSTX.iframe.contentWindow.postMessage(JSON.stringify({action:"setComparisons", comparisons:comparisons}), "*");
            };
            EDGRSTX.initialize({container:"chart-container", symbol: "AAPL", comparison:[], range:"1D"});
        });
