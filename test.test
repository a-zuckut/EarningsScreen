//ad refresh
   
(function () {
    //array holding indexes into gSlots array of ads to be refreshed
    var refreshableIndexes = [];
    //tracks the number of premake blocks which fail to render an ad
    //need this because refreshableIndex array values will be used as indexes to gSlots array, where failed blocks won't exist
    var failIndex = 0;

    function evalBlock(adBlock, index) {
        if (adBlock.label == "ad") {
            if (adBlock.site == "btf")
                refreshableIndexes.push(index);
        }
        if (adBlock.label == "condition") {
            if (adBlock.expression != "adRotatorRandom" && adBlock.expression != "adRotatorRandomSwitch") {
                if (adBlock.expression == true)
                    evalBlock(adBlock["true"], index);
                else if (adBlock.hasOwnProperty("false"))
                    evalBlock(adBlock["false"], index);
                else
                    failIndex++;//block failed to render an ad
            }
            else {
                if (adBlock.expression == "adRotatorRandom") {
                    if (adRotatorRandStore[adBlock.randIndex])
                        evalBlock(adBlock['true'], index);
                    else if (adBlock.hasOwnProperty("false"))
                        evalBlock(adBlock["false"], index);
                    else
                        failIndex++;//block failed to render an ad
                }
                else {//eval the block at the index resulting from the random switch  
                    evalBlock(adBlock[adRotatorRandStore[adBlock.randIndex]], index);
                }
            }
        }
    }

    premakeAds.forEach(function (adBlock, index) {
        evalBlock(adBlock, index - failIndex);
    });

    googletag.cmd.push(function () {
        autoAdRefresh(refreshableIndexes, 60000);
    });
})();

function elementInViewport(el) 
{
    var rect = el.getBoundingClientRect()
    return (rect.top >= 0 && rect.left >= 0 && rect.top <= (window.innerHeight || document.documentElement.clientHeight));
}

function postHighChartsLoad()
