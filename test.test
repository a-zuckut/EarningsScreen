<script language="javascript" type="text/javascript">
    $(document).ready(function () {
        setTimeout(function () {
            initStickyContent();
        }, 5000);

    });

    function initStickyContent() {
        initStickyContent1();
        //initStickyContent2();
    }

    function initStickyContent1() {
        var stickyContainer = $('#sticky-container1');
        if (stickyContainer.length == 0) return;

        var stickyTop = stickyContainer.offset().top - 85;
        var sheight = stickyContainer.height();
        var bottom = $('#footerAd').offset().top - 120;
        //var bottom = 940;
        var distance = bottom - stickyTop - sheight;
        var currentlyStickied = false;
        var disableSticky = false;
        $(window).scroll(function () {
            var windowTop = $(window).scrollTop();
            if (!disableSticky) {
                if (stickyTop < windowTop) {
                    if ((windowTop + sheight) < bottom) {
                        if (!currentlyStickied) {
                            stickyContainer.css({ position: 'fixed', top: "85px" });
                            currentlyStickied = true;
                        }
                    }
                    else {
                        if (currentlyStickied) {
                            stickyContainer.css('position', 'static');
                            currentlyStickied = false;
                            disableSticky = true;
                        }
                    }
                }
                else {
                    if (currentlyStickied) {
                        stickyContainer.css('position', 'static');
                        currentlyStickied = false;
                    }
                }
            }
        });

    }

    function initStickyContent2() {
        var stickyContainer = $('#sticky-container2');
        if (stickyContainer.length == 0) return;

        var stickyTop = stickyContainer.offset().top - 85;
        var sheight = stickyContainer.height();
        var bottom = $('#footerAd').offset().top - 120;
        var distance = bottom - stickyTop - sheight;
        var currentlyStickied = false;
        $(window).scroll(function () {
            var windowTop = $(window).scrollTop();
            if (stickyTop < windowTop) {
                if ((windowTop + sheight) < bottom) {
                    if (!currentlyStickied) {
                        stickyContainer.css({ position: 'fixed', top: "85px" });
                        currentlyStickied = true;
                    }
                }
                else {
                    if (currentlyStickied) {
                        stickyContainer.css({ position: 'relative', top: distance + "px" });
                        currentlyStickied = false;
                    }
                }
            }
            else {
                if (currentlyStickied) {
                    stickyContainer.css('position', 'static');
                    currentlyStickied = false;
                }
            }
        });
    }
var sleeptime = 7; var today = new Date(); var timerid = null; var xmlhttp = null;
if (today.getDay() == 6 || today.getDay() == 0 || today.getHours() < 9 || today.getHours() > 17)
{ sleeptime = 9; /*Sleep time during non business hours*/ }
else
{ sleeptime = 7; /*Sleep time during business hours */ }
function setCookie(c_name, value, expiredays) { var exdate = new Date(); exdate.setDate(exdate.getDate() + expiredays); document.cookie = c_name + "=" + escape(value) + ((expiredays == null) ? "" : ";expires=" + exdate.toGMTString()); }
function getCookie(c_name) { if (document.cookie.length > 0) { c_start = document.cookie.indexOf(c_name + "="); if (c_start != -1) { c_start = c_start + c_name.length + 1; c_end = document.cookie.indexOf(";", c_start); if (c_end == -1) c_end = document.cookie.length; return unescape(document.cookie.substring(c_start, c_end)); } } return ""; }
function getQuoteData() {
    var Streaming = getCookie('streamingdata');
    if (Streaming == "On") {
        document.getElementById('DynamicSwitch').innerHTML = "Turn Off";
        setCookie('streamingdata', 'On', 365);
        getData();
    }
    else {
        document.getElementById('DynamicSwitch').innerHTML = "Turn On";
        setCookie('streamingdata', 'Off', 365);
    }
}
function getData() {
    if (window.XMLHttpRequest) {
        xmlhttp = new XMLHttpRequest();
    }
    else if (window.ActiveXObject) {
        xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
    }
    if (xmlhttp == null) {
        alert("Browser does not support dynamic data");
    }
    else {
        var Url = "../quotedll/quote.dll?page=InfoQuotes&mode=stock&symbol=AAPL&selected=AAPL&" + Math.random();
        xmlhttp.onreadystatechange = UpdateQuoteData;
        xmlhttp.open("GET", Url, true);
        xmlhttp.send(null);
        timerid = self.setTimeout("getQuoteData()", sleeptime * 1000);
    }
}
function UpdateQuoteData() {
    if (xmlhttp.readyState == 4 || xmlhttp.readyState == "complete") {
        if (xmlhttp.responseText != "") {
            var symboldata = new Array();
            var changesign = "";
            var prepcl = "";
            symboldata = xmlhttp.responseText.split("<*>");
            for (var i = 1; i < symboldata.length; i++) {
                var symbolvalues = new Array();
                symbolvalues = symboldata[i].split("|");

                // Update the Volume
                document.getElementById(symbolvalues[0] + "_Volume").innerHTML = symbolvalues[6];
                // Market Status
                SetQuoteBarTimeStamp(symbolvalues[26]);

                if (symbolvalues[2].toUpperCase() != 'OTCBB' && symbolvalues[2].toUpperCase() != 'OTHER OTC') {
                    SetQuoteBarText(symbolvalues[5], symbolvalues[22], symbolvalues[23], symbolvalues[24]);
                    document.title = symbolvalues[0] + " : $" + symbolvalues[46].toString() + changesign + symbolvalues[22] + " (" + symbolvalues[23] + ")";
                } else {
                    var netch = "";
                    var pctch = "";
                    var dispnet = "";
                    var dispct = "";

                    prepcl = 
                    netch = parseFloat(symbolvalues[46] - prepcl);

                    pctch = netch / prepcl;
                    dispnet = parseFloat(netch).toFixed(4);
                    dispct = (pctch * 100).toFixed(2) + "%";

                    if (parseFloat(netch) > 0) {
                        SetQuoteBarText(symbolvalues[5], dispnet, dispct, "up");
                        changesign = "+";
                    }
                    else if (parseFloat(netch) < 0) {
                        SetQuoteBarText(symbolvalues[5], dispnet, dispct, "down");
                        changesign = "-";
                    }
                    else {
                        SetQuoteBarText(symbolvalues[5], dispnet, dispct, "unch");
                        dispnet = "unch";
                    }
                    if (netch == "0.0000") {
                        SetQuoteBarText(symbolvalues[5], dispnet, dispct, "unch");
                        dispnet = "unch";
                    }
                    if (symbolvalues[6] == "0") {
                        document.title = symbolvalues[0] + " : $" + prepcl + " (unch)";
                        SetQuoteBarText(" : $" + prepcl, "unch", "unch", "unch");
                        dispnet = "unch";
                    }
                    else {
                        document.title = symbolvalues[0] + " : $" + symbolvalues[46].toString() + changesign + dispnet + " (" + dispct + ")";
                    }
                }
            }
        }
    }
}
function StreamingClick() {
    var value = document.getElementById('DynamicSwitch').innerHTML;
    if (value == 'Turn On') {
        document.getElementById('DynamicSwitch').innerHTML = "Turn Off";
        setCookie('streamingdata', 'On', 365);
        getQuoteData();
    }
    else {
        document.getElementById('DynamicSwitch').innerHTML = "Turn On";
        setCookie('streamingdata', 'Off', 365);
        clearTimeout(timerid);
    }
}
if (document.getElementById('DynamicSwitch') != null)
    timerid = self.setTimeout("getQuoteData()", sleeptime * 1000);
</script>
