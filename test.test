$(function(){
    var chartContainerArray=[];
    var charts_increment = 0;
    var charts_elements = document.getElementById('left-column-div').getElementsByClassName('highChartContainer');

    function charts_incrementID()
    {
        var charts_increment = 0;
        var charts_elements = document.getElementById('left-column-div').getElementsByClassName('highChartContainer');
        for (; charts_increment < charts_elements.length; charts_increment++)
        {
            charts_elements[charts_increment].setAttribute('id', 'highChartContainer-'+eval(charts_increment+1));
        }
    }

    charts_incrementID();

    for (; charts_increment < charts_elements.length; charts_increment++)
    {
        chartContainerArray.push(document.getElementById('highChartContainer-'+eval(charts_increment+1)));
    }

    setTimeout(function()
    {
        checkHighCharts(chartContainerArray);
    },
	500);
});

function checkHighCharts(chartContainerArray)
{
    var numElements = chartContainerArray.length;
    var i;
    var lp = location.protocol;
    for(i=0;i<numElements;i++)
    {
        if(elementInViewport(chartContainerArray[i]))
        {
            addScript(lp + "//code.highcharts.com/highcharts.js", true, "postHighChartsLoad()");
            addStyleSheet(lp + "//old.nasdaq.com/includes/highcharts-styles.min.css");
            return;
        }
    }
    setTimeout(function()
    {
        checkHighCharts(chartContainerArray);
    },
	500);
}

